<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="-95%"
            android:pivotY="100%">
            <shape android:shape="rectangle">
                <size
                    android:width="50dp"
                    android:height="50dp" />
                <solid android:color="@color/popBackground" />
            </shape>
        </rotate>
    </item>
</layer-list>
